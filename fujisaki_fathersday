    <script src="/d_images/js/jquery.matchHeight-min.js?releaseVer=2503171935"></script>

    <script>

      $(function() {
        var swiperSlide = new Swiper(".swiper", {
          //centeredSlides: true,
          slidesPerView: 3,
          //slidesPerGroup: 3,
          spaceBetween: 10,
          loop: true,
          loopAdditionalSlides: 1,
          navigation: {
            nextEl: ".nextItem",
            prevEl: ".prevItem",
          },
          breakpoints: {
            880: {
                slidesPerView: 2,
                spaceBetween: 15
              },
              767: {
                slidesPerView: 2,
                spaceBetween: 12
              },
          }
        });
      });

      $(function() {
        $('.lists .itemLead').matchHeight();
        $('.lists .itemName').matchHeight();
        $('.lists .detailSet').matchHeight();
      });

    // *スワイパーの矢印の高さ調整(ブラウザ幅でも変動)*
    function adjustButtonPosition() {
    const swiperSections = document.querySelectorAll('.swiper-sec');

      swiperSections.forEach(section => {
          const slides = section.querySelectorAll('.swiper-slide');
          const prevButton = section.querySelector('.swiper-button-prev');
          const nextButton = section.querySelector('.swiper-button-next');
          
          slides.forEach(slide => {
              const itemImg = slide.querySelector('.itemImg');
              if (itemImg) {
                  // itemImgの高さを取得
                  const itemImgHeight = itemImg.offsetHeight;

                  // ボタンの位置を調整
                  prevButton.style.top = `${(itemImgHeight / 2) - (prevButton.offsetHeight / 2)}px`;
                  nextButton.style.top = `${(itemImgHeight / 2) - (nextButton.offsetHeight / 2)}px`;
              }
          });
      });
    }
    window.addEventListener('load', adjustButtonPosition);
    window.addEventListener('resize', adjustButtonPosition);
    </script>

